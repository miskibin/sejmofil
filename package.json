{
  "name": "sejmofront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "prettier": "prettier . --write",
    "husky:prepare": "pnpm husky && pnpm husky:add-precommit && pnpm husky:add-prepush",
    "husky:add-precommit": "powershell -Command \"New-Item -Path .husky/pre-commit -ItemType File -Force; Set-Content -Path .husky/pre-commit -Value 'pnpm lint-staged && pnpm lint && echo Committing finished'\"",
    "husky:add-prepush": "powershell -Command \"New-Item -Path .husky/pre-push -ItemType File -Force; Set-Content -Path .husky/pre-push -Value 'pnpm lint && echo Pushing finished'\""
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.tsx": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "@tailwindcss/postcss": "^4.1.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "debounce": "^2.2.0",
    "embla-carousel-react": "^8.5.1",
    "framer-motion": "^11.15.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.468.0",
    "neo4j-driver": "^5.27.0",
    "next": "15.5.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-force-graph-2d": "^1.29.0",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.1",
    "recharts": "^2.15.0",
    "rehype-raw": "^7.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
